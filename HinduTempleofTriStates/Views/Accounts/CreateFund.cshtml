@model HinduTempleofTriStates.Models.Fund

<h2>Create Fund</h2>

<form asp-action="CreateFund" method="post">
    <div class="form-group">
        <label asp-for="AccountName"></label>
        <input asp-for="AccountName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Balance"></label>
        <input asp-for="Balance" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
